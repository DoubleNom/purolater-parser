{
  "name": "Purolator Parser",
  "description": "Automatically fill address into purolator webform",
  "version": "0.3",
  "manifest_version": 3,
  "icons": {
    "16": "/icons/favicon_16.png",
    "32": "/icons/favicon_32.png",
    "48": "/icons/favicon_48.png"
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "http://localhost/index.html",
    "https://eshiponline.purolator.com/ShipOnline/shipment/shipment.aspx"
  ],
  "background": {
    "service_worker": "/src/background.js"
  },
  "action": {
    "default_popup": "/src/popup.html",
    "default_icons": {
      "16": "/icons/favicon_16.png",
      "32": "/icons/favicon_32.png",
      "48": "/icons/favicon_48.png"
    }
  }
}